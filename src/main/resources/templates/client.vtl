<div class="center">
  <h2>$client.getName()</h2>
  <div class="editIcons">
    <a href="/stylists/$stylist.getId()/clients/$client.getId()/edit"><button type="submit"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></button></a>
    <a href="/stylists/$stylist.getId()/clients/$client.getId()/delete"><button type="submit"><i class="fa fa-trash-o" aria-hidden="true"></i></button></a>
